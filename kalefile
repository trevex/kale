project("simple")

kubectl = require("kubectl", { "version": ">=v1.10.0" })

def build(params):
    print(params)
    kubectl.apply("test")
    print("last")

target(build, params = {
    "values": schema.filename(required = True),
    "version": schema.string(default = "0.9.0"),
    "b": schema.bool(default = True),
    "f": schema.float(default = 1.0),
    "i": schema.int(default = "x")
})
